<script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>

<% if (typeof needDateTime !== 'undefined' && needDateTime) { %>
<script src="/js/jquery.datetimepicker.full.min.js"></script>
<script type="text/javascript">
    $(function () {
        $('#isEnded').change(function () {
            if ($(this).is(":checked")) {
                $('#endDateString').removeAttr('disabled');
                $('#isSuccess').removeAttr('disabled');
                $('#endDateOption').show();
            } else {
                $('#endDateString').attr('disabled', 'disabled');
                $('#isSuccess').attr('disabled', 'disabled');
                $('#endDateOption').hide();
            }
        });
    });
    $(function () {
        $('#startDateString').datetimepicker();
    });
    $(function () {
        $('#endDateString').datetimepicker();
    });
</script>
<% } %>

<% if (typeof userID !== 'undefined') { %>
<script type="text/javascript">
    function askDelete() {
        if (confirm("Are you sure you want to delete your account on DevLabs?")) {
            window.location.href = "/account/delete";
        }
    }
</script>
<% } %>

</body>
</html>